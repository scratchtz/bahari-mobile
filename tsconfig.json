{
    "extends": "expo/tsconfig.base",
    "compilerOptions": {
        "strict": true,
        "types": ["jest", "node"],
        "paths": {
            "@components/*": ["./components/*"],
            "@utils/*": ["./utils/*"],
            "@assets/*": ["./assets/*"],
            "@navigation/*": ["./navigation/*"],
            "@screens/*": ["./screens/*"],
            "@storage/*": ["./storage/*"],
            "@state/*": ["./state/*"],
            "@hooks/*": ["./hooks/*"],
            "@constants/*": ["./constants/*"],
        }
    }
}
